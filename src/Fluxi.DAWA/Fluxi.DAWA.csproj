<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Version>1.0.0</Version>
    <FileVersion>1.0.0</FileVersion>
    <Authors>Fluxi</Authors>
    <Description>SDK for DAWA</Description>
    <Copyright>Copyright (c) 2018 Fluxi</Copyright>
  </PropertyGroup>

  <PropertyGroup>
    <PackageId>Fluxi.DAWA</PackageId>
    <PackageVersion>0.0.1</PackageVersion>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <PackageTags>DAWA SDK</PackageTags>
    <PackageProjectUrl>https://github.com/fluxidk/Fluxi.DAWA</PackageProjectUrl>
  </PropertyGroup>

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;net452</TargetFrameworks>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <RepositoryUrl>https://github.com/fluxidk/Fluxi.DAWA</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="RestSharp" Version="106.3.1" />
  </ItemGroup>

</Project>
